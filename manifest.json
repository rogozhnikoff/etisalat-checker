{
  "name": "ГдеМойИнтернетЧувак?",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Бегает на шри ланкийский etisalat и проверяет сколько осталось",
  "icons": {
    "16": "icons/etisalat-logo.png",
    "48": "icons/etisalat-logo.png",
    "128": "icons/etisalat-logo.png"
  },
  "browser_action": {
    "default_icon": "icons/etisalat-logo-icon.png"
  },
  "background": {
    "scripts": [
      "src/jquery.min.js",
      "src/index.js"
    ],
    "persistent": true
  },
  "permissions": [
    "notifications",
    "webRequest",
    "https://mypage.etisalat.lk/bbportal/home"
  ]
}